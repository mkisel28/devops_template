# Production Environment Configuration
# Registry settings
REGISTRY_PORT=5000
REGISTRY_UI_PORT=8080
TRIVY_PORT=8081

# Nginx settings
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# Registry configuration
